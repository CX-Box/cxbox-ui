{
    "compilerOptions": {
        "target": "es5",
        "module": "es2015",
        "moduleResolution": "node",
        "baseUrl": "../../src",
        "noImplicitReturns": true,
        "noImplicitAny": true,
        "lib": [
            "dom",
            "es2017",
        ],
        "allowSyntheticDefaultImports": true,
        "types": [],
        "typeRoots": [],
        "strict": true,
        "noEmit": true,
        "forceConsistentCasingInFileNames": true
    }
}
